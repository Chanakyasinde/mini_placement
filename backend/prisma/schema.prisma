generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Students {
  student_id       Int      @id @default(autoincrement())
  student_name     String
  password         String
  phone_number     BigInt   @unique
  email            String   @unique
  resume_link      String
}

model companies {
  companyId      Int      @id @default(autoincrement())
  companyName    String   @unique
  email          String   @unique
  password       String?
  websiteUrl     String?
  companyType    String?
  industry       String?
  location       String?
  createdAt      DateTime @default(now())
}


model Admins {
  admin_id    Int      @id @default(autoincrement())
  admin_name  String
  email       String   @unique
  createdAt   DateTime @default(now())
}
